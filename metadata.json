{
  "name": "puppetlabs-openstack",
  "version": "8.0.0",
  "author": "Puppet Labs and Community Contributors, with initial work and subsequent fork by Chris Hoge",
  "summary": "Deploy various types of installations of OpenStack. Let's not kid ourselves, if you're packaging the module, you're doing it wrong.",
  "license": "Apache License, Version 2.0",
  "source": "https://github.com/hogepodge/puppetlabs-openstack.git",
  "project_page": "https://github.com/hogepodge/puppetlabs-openstack",
  "issues_url": "https://github.com/hogepodge/puppetlabs-openstack/issues",
  "requirements": [
    { "name": "puppet","version_requirement": "3.x" }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["7"]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["14.04"]
    }
  ],
  "description": "Using a hiera-based roles and profiles model, do simple OpenStack deployments.",
  "dependencies": [
    { "name": "openstack/ceilometer", "version_requirement": ">=8.0.0 <9.0.0" },
    { "name": "openstack/cinder",     "version_requirement": ">=8.0.0 <9.0.0" },
    { "name": "openstack/glance",     "version_requirement": ">=8.0.0 <9.0.0" },
    { "name": "openstack/heat",       "version_requirement": ">=8.0.0 <9.0.0" },
    { "name": "openstack/horizon",    "version_requirement": ">=8.0.0 <9.0.0" },
    { "name": "openstack/keystone",   "version_requirement": ">=8.0.0 <9.0.0" },
    { "name": "openstack/neutron",    "version_requirement": ">=8.0.0 <9.0.0" },
    { "name": "openstack/nova",       "version_requirement": ">=8.0.0 <9.0.0" },
    { "name": "openstack/swift",      "version_requirement": ">=8.0.0 <9.0.0" },
    { "name": "openstack/vswitch",    "version_requirement": ">=0.2.0 <1.0.0" },

    { "name": "puppetlabs/firewall",  "version_requirement": ">=1.0.0 <2.0.0" },
    { "name": "puppetlabs/ntp",       "version_requirement": ">=3.0.0 <4.0.0" },
    { "name": "puppetlabs/mongodb",   "version_requirement": ">=0.6.0 <1.0.0" },
    { "name": "puppetlabs/mysql",     "version_requirement": ">=3.0.0 <4.0.0" },
    { "name": "puppetlabs/rabbitmq",  "version_requirement": ">=5.0.0 <6.0.0" },
    { "name": "puppetlabs/apache",    "version_requirement": ">=1.0.0 <2.0.0" },

    { "name": "duritong/sysctl",      "version_requirement": ">=0.0.1 <1.0.0" }
  ]
}
